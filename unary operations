class prac2{
	public static void main(String args[]) {
		int x=10;
		int y=10;
		int z=10;
		int a=10;
		System.out.println(x*x);
		System.out.println(x);
		System.out.println(++x);
		System.out.println(x++);
		System.out.println(--x);
		System.out.println(x--);
		System.out.println(x++);//10 (11)
		System.out.println(x);
		System.out.println(++y);//12  
		System.out.println(z--);//12 (11)  
		System.out.println(--a);//10  
		
		
		
		
	}
}
