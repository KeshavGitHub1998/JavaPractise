class prac2{
	public static void main(String args[]) {
		System.out.println("Positive negative or zero ");
		int number = 23;
		if(number==0) {
			System.out.println("the number is zero");
			
		}else if(number>0){
			System.out.println("positive");
			
			
		}else {
			System.out.println("negative");
			
		}
		
	}
}
